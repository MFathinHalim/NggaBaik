{
  "manifest_version": 3,
  "name": "Project 32",
  "version": "0.5",
  "description": "Monitor search bar inputs.",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
    "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
